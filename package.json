{
  "name": "rag-n-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "deploy-sbx": "npx ampx sandbox --outputs-format dart",
    "gen-models": "rm -rf ./src/models && npx ampx generate graphql-client-code --format modelgen --model-target javascript --out ./src/models",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "aws-amplify": "^6.15.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.5.2",
    "aws-cdk": "^2.1018.1",
    "aws-cdk-lib": "^2.201.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
